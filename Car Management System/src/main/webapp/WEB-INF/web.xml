    <display-name>Archetype Created Web Application</display-name>


    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.pls.cms.servlet.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CarSearchServlet</servlet-name>
        <servlet-class>com.pls.cms.servlet.CarServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CarSearchServlet</servlet-name>
        <url-pattern>/searchCar</url-pattern>
    </servlet-mapping>


